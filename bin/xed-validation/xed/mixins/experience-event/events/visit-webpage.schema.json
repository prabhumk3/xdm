{
    "$id": "mixins/events/visit-webpage.schema.json",
    "$schema": "http://json-schema.org/draft-06/schema#",
    "title": "Visit WebPage",
    "type": "object",
    "meta:conditionalField": "xdm:eventType",
    "meta:conditionalValue": "web.webpagedetails.pageViews",
    "description": "Use for information regarding web details events such as interaction, page details, and referrer.",
    "meta:status": "experimental",
    "meta:xdmType": "object",
    "properties": {
        "web": {
            "title": "Web",
            "type": "object",
            "meta:xdmType": "object",
            "properties": {
                "webPageDetails": {
                    "title": "Web Page Details",
                    "type": "object",
                    "meta:xdmType": "object",
                    "properties": {
                        "webPageID": {
                            "title": "Webpage ID",
                            "type": "string",
                            "description": ".",
                            "meta:xdmType": "string",
                            "meta:xdmField": "xdm:webPageID",
                            "tagNumber": 1
                        },
                        "name": {
                            "title": "Name",
                            "type": "string",
                            "description": "The normative name of the web page. This name is not necessarily the page title or directly associate with page content, but is used to organize a site's pages for classification purposes.",
                            "meta:xdmType": "string",
                            "meta:xdmField": "xdm:name",
                            "tagNumber": 2
                        },
                        "URL": {
                            "title": "URL",
                            "type": "string",
                            "format": "uri",
                            "description": "The normative or usual URL of the web page.  This may or may not be the actual URL used to reach the page, which would be recorded using `Web Link`.",
                            "meta:xdmType": "string",
                            "meta:xdmField": "xdm:URL",
                            "tagNumber": 3
                        },
                        "queryParamaters": {
                            "title": "Query Paramaters",
                            "type": "string",
                            "description": "Query Paramters used for the web page.",
                            "meta:xdmType": "string",
                            "meta:xdmField": "xdm:queryParamaters",
                            "tagNumber": 4
                        },
                        "isPersonalizedURL": {
                            "title": "Is Personalized URL",
                            "type": "boolean",
                            "description": "Personalized URL of the web page.  This may or may not be the actual URL used to reach the page, which would be recorded using `Web Link`.",
                            "meta:xdmType": "boolean",
                            "meta:xdmField": "xdm:isPersonalizedURL",
                            "tagNumber": 5
                        }
                    },
                    "meta:xdmField": "xdm:webPageDetails",
                    "tagNumber": 1
                },
                "webReferrer": {
                    "title": "Web Referrer",
                    "type": "object",
                    "meta:xdmType": "object",
                    "properties": {
                        "URL": {
                            "title": "URL",
                            "type": "string",
                            "description": "The referrer URL.",
                            "meta:xdmType": "string",
                            "meta:xdmField": "xdm:URL",
                            "tagNumber": 1
                        }
                    },
                    "meta:xdmField": "xdm:webReferrer",
                    "tagNumber": 2
                }
            },
            "meta:xdmField": "xdm:web",
            "tagNumber": 1
        },
        "environment": {
            "title": "Environment",
            "type": "object",
            "description": "The information related to web page and link of the ExperienceEvent.",
            "meta:xdmType": "object",
            "properties": {
                "ipV4": {
                    "title": "IPv4",
                    "type": "string",
                    "format": "ipv4",
                    "description": "The numerical label assigned to a device participating in a computer network that uses the Internet Protocol for communication. ",
                    "meta:xdmType": "string",
                    "meta:xdmField": "xdm:ipV4",
                    "tagNumber": 1
                },
                "browserDetails": {
                    "title": "Browser details",
                    "type": "object",
                    "description": "The browser specific details such as browser name, version, javascript version, user agent string, and accept language.",
                    "meta:xdmType": "object",
                    "properties": {
                        "userAgent": {
                            "title": "User agent",
                            "type": "string",
                            "description": "The HTTP user-agent string from the client request.",
                            "meta:xdmType": "string",
                            "meta:xdmField": "xdm:userAgent",
                            "tagNumber": 1
                        }
                    },
                    "meta:xdmField": "xdm:browserDetails",
                    "tagNumber": 2
                }
            },
            "meta:xdmField": "xdm:environment",
            "tagNumber": 2
        },
        "search": {
            "title": "Search",
            "type": "object",
            "description": "The information related to web or mobile search.",
            "meta:xdmType": "object",
            "properties": {
                "searchEngine": {
                    "title": "Search engine",
                    "type": "string",
                    "description": "The search engine used by the search.",
                    "meta:xdmType": "string",
                    "meta:xdmField": "xdm:searchEngine",
                    "tagNumber": 1
                },
                "keywords": {
                    "title": "Keywords",
                    "type": "string",
                    "description": "Keywords for the search.",
                    "meta:xdmType": "string",
                    "meta:xdmField": "xdm:keywords",
                    "tagNumber": 2
                }
            },
            "meta:xdmField": "xdm:search",
            "tagNumber": 3
        }
    },
    "meta:xdmId": "https://ns.adobe.com/xdm/mixins/events/visit-webpage",
    "meta:altId": "_xdm.mixins.events.visit-webpage"
}